        "ramda": "^0.30.1",
        "ringcentral-open-api-parser": "^0.15.3"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.13.1",
        "@types/ramda": "^0.30.2",
        "dotenv-override-true": "^6.2.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3",
        "yarn-upgrade-all": "^0.7.4"
    },
    "yarn-upgrade-all": {
        "ignore": [
